        //airlineData[i] = new Airline(airline, availSeat, inc8599, fatal8599, fatalities8599, inc0014, fatal0014, fatalities0014);

    for (int i=0; i<table.getRowCount;i++){
        ellipse(inc8599,inc0014,availSeat/1000,availSeat/1000);
    }
  }

}

