Table tab;

void setup(){
    size(800,800);
    tab=loadTable("airlineSafety.csv","header");// if the data has a header row, use "header"
    println(tab.getRowCount() + " total rows in table");
    String [] airline = new String[table.getRowCount];
    int [] availSeat = new int[table.getRowCount];
    int [] inc8599 = new int[table.getRowCount];
    int [] fatal8599 = new int[table.getRowCount];
    int [] fatalities8599 = new int[table.getRowCount];
    int [] inc0014 = new int[table.getRowCount];
    int [] fatal0014 = new int[table.getRowCount];
    int [] fatalities0014 = new int[table.getRowCount];
    

    for (int i=0; i<table.getRowCount; i++) {
        TableRow row = table.getRow(i);
